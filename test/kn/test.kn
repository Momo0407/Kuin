func main()
	var xml: xml@Xml :: xml@makeXml("res/test.xml")
	do xml.head()
	do cui@print(xml.len().toStr() ~ "\n")
	class c()
		+var ss: []char
	end class

	var c2: c :: #c
	do c2.ss :: ""
	do cui@print(xml.forEach(callback, c2).toStr() ~ "\n")
	do cui@print(c2.ss ~ "\n")

	func callback(idx: int, key: []char, c2: c): bool
		do cui@print(idx.toStr() ~ ": " ~ key ~ "\n")
		do c2.ss :~ key
		ret true
	end func
end func
