func main()
	block
		var a: big_num@BigInt :: big_num@makeBigInt()
		do a.fromInt(5)
		do a.subInt(3).mulInt(2)
		var b: big_num@BigInt :: big_num@makeBigInt()
		do b.fromStr("0x10")
		do b.div(a)
		do cui@print(a.toStr())
		do cui@print(b.toStr())
		do cui@print((a = b).toStr())
		do cui@print((a < b).toStr())
		do cui@print((a > b).toStr())
	end block

	block
		var a: big_num@BigFloat :: big_num@makeBigFloat()
		do a.fromFloat(5.0)
		do a.subFloat(3.0).mulFloat(2.0)
		var b: big_num@BigFloat :: big_num@makeBigFloat()
		do b.fromStr("7")
		do a.div(b)
		do cui@print(a.toStr())
		do cui@print(b.toStr())
		do cui@print((a = b).toStr())
		do cui@print((a < b).toStr())
		do cui@print((a > b).toStr())
	end block
end func
