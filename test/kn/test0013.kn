func main()
	for(1, 3)
		block
			var a: []int :: #[3]int
			do a[0] :: 1
			do a[1] :: 2
			do a[2] :: 3
			do cui@print((^a).toStr() ~ ", " ~ a[0].toStr() ~ ", " ~ a[1].toStr() ~ ", " ~ a[2].toStr())
		end block

		block
			var a: [][][]char :: #[3, 2, 4]char
			do cui@print((^a).toStr() ~ ", " ~ (^a[0]).toStr() ~ ", " ~ (^a[0][0]).toStr())
		end block

		block
			var x: float :: 314159.0e-5
			do cui@print(x.toStr())
		end block
	end for
end func
